{
  "name": "verum-omnis-web",
  "version": "5.2.7",
  "description": "Verum Omnis Forensic Engine - Web & Capacitor Application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios",
    "cap:sync": "cap sync",
    "cap:build:android": "cap open android",
    "cap:build:ios": "cap open ios",
    "cap:run:web": "npm run build && cap serve",
    "android:build": "npm run build && cap sync android && npm run cap:build:android",
    "ios:build": "npm run build && cap sync ios && npm run cap:build:ios",
    "web:build": "npm run build",
    "web:serve": "npm run serve",
    "clean": "rm -rf dist node_modules",
    "verify:apk": "node scripts/verify-apk.js",
    "check:security": "npm audit",
    "prebuild": "npm run type-check && npm run lint"
  },
  "keywords": [
    "forensic",
    "legal",
    "constitutional",
    "evidence",
    "document-processing",
    "capacitor"
  ],
  "author": "Verum Omnis Contributors",
  "license": "Apache-2.0",
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/react": "^7.4.0",
    "@ionic/react-router": "^7.4.0",
    "@pdfjs/dist": "^4.0.379",
    "cryptojs": "^1.0.6",
    "date-fns": "^2.30.0",
    "firebase": "^10.7.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lodash-es": "^4.17.21",
    "pdfkit": "^0.13.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.13.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
